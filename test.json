{
    "scalestack": {
        "logging": {
        "version": 1,
        "formatters": {
            "default": {
                "format": "%(asctime)s %(levelname)s %(name)s %(message)s"}},
        "handlers": {
            "file": {
                "class": "logging.FileHandler",
                "formatter": "default",
                "filename": "test.log",
                "level": "DEBUG"}},
        "root": {
            "handlers": ["file"]}}},

    "scalestack.http": {
        "host": "127.0.0.1",
        "port": 8123},

    "scalestack.file": {
        "map": [
            {"file_path": "./"}]}
}
